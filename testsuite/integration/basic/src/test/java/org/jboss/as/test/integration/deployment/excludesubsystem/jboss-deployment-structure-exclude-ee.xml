<jboss-deployment-structure xmlns="urn:jboss:deployment-structure:1.2">
    <deployment>
        <exclude-subsystems>
            <subsystem name="ejb3" />
            <subsystem name="ee" />
            <subsystem name="webservices" />
            <subsystem name="weld" />
            <subsystem name="messaging-activemq" />
            <subsystem name="jpa" />
            <subsystem name="undertow" />
        </exclude-subsystems>
        <exclusions>
            <module name="jakarta.jms.api"></module>
        </exclusions>
    </deployment>
</jboss-deployment-structure>

