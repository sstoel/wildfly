        version="4.0">

    <jakartaee:assembly-descriptor>
        <mdb:resource-adapter-binding>
            <jakartaee:ejb-name>NonJMSMDBRelativePathWithDeploymentDescriptor</jakartaee:ejb-name>
            <mdb:resource-adapter-name>#rar-within-a-ear.rar</mdb:resource-adapter-name>
        </mdb:resource-adapter-binding>
    </jakartaee:assembly-descriptor>

</ejb-jar>
