<module xmlns="urn:jboss:module:1.9" name="org.jboss.resteasy.resteasy-client">

    <properties>
        <property name="jboss.api" value="private"/>
    </properties>

    <resources>
        <artifact name="${org.jboss.resteasy:resteasy-client}" />
    </resources>

    <dependencies>
        <module name="java.logging"/>
        <module name="java.management"/>
        <module name="jakarta.ws.rs.api"/>
        <module name="org.apache.commons.codec" />
        <module name="org.apache.httpcomponents"/>
        <module name="org.jboss.logging"/>
        <module name="org.jboss.resteasy.resteasy-client-api"/>
        <module name="org.jboss.resteasy.resteasy-core" services="import"/>
        <module name="org.jboss.resteasy.resteasy-core-spi"/>
        <module name="org.reactivestreams"/>
    </dependencies>
</module>
